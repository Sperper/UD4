                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="nombre"></xs:element>
                            <xs:element name="contenidos">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="UD">
                                            <xs:complexType>
                                                <xs:sequence>
